[
  {
    "name": "frontend",
    "image": "webatspeed/webatspeed-fe:0.4.0",
    "cpu": 256,
    "memory": 512,
    "portMappings": [
      {
        "containerPort": ${port}
      }
    ],
    "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
        "awslogs-group": "${log_group}",
        "awslogs-region": "${region}",
        "awslogs-stream-prefix": "frontend"
      }
    }
  }
]
